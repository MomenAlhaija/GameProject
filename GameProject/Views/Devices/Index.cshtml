@model IEnumerable<DeviceDTO>
@if(Model is null){
    <p>
        No Any Category To Display;
    </p>
}
else{
    <table>
        <thead>
            <th>Name</th>
            <th>Description</th>
            <th>Device Icon</th>
        </thead>
        <tr>
        @foreach (var item in Model)
        {
            <td>@item.Name</td>
            <td><i class="@item.DeviceIcon me-2 fs-4"></i></td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>  
        }
        </tr>
    </table>

}
<a asp-controller="Devices" asp-action="Add">Add Device</a>